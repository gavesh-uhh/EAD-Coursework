	<query language="SQL"><![CDATA[SELECT
    s.full_name AS StudentName,
    sub.subject_name AS Subject,
    m.exam_type AS ExamType,
    m.marks_obtained AS MarksObtained,
    m.max_marks AS MaxMarks,
    m.exam_date AS ExamDate
FROM
    students s
JOIN
    marks m ON s.student_id = m.student_id
JOIN
    subjects sub ON m.subject_id = sub.subject_id
ORDER BY
    s.full_name, sub.subject_name, m.exam_date]]></query>
