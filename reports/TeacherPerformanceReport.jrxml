	<query language="SQL"><![CDATA[SELECT
    t.full_name AS TeacherName,
    sub.subject_name AS SubjectTaught,
    AVG(m.marks_obtained) AS AverageMarks
FROM
    teachers t
JOIN
    subjects sub ON t.teacher_id = sub.teacher_id
JOIN
    marks m ON sub.subject_id = m.subject_id
GROUP BY
    t.full_name, sub.subject_name
ORDER BY
    AverageMarks DESC]]></query>
